{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    },
    {
      "src": "assets-foxiz/**",
      "use": "@vercel/static"
  }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.+)",
      "dest": "assets-foxiz/$1"
  }
  ]
}
